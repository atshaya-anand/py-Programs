   "source": [
    "# program to check whether a number is palindrome\n",
    "\n",
    "num = input('Enter a number:')\n",
    "n = int(num)\n",
    "nCopy = n\n",
    "palNum = 0\n",
    "\n",
    "while(n):\n",
    "    i = n%10\n",
    "    palNum = palNum*10+i\n",
    "    n = n//10\n",
    "    \n",
    "if(palNum == nCopy):\n",
    "    print('Palindrome')\n",
    "else:\n",
    "    print('Not a Palindrome')"
   ]
