   "source": [
    "# program to swap 2 numbers in a variable\n",
    "\n",
    "num1 = input('Enter 1st number:')\n",
    "a = int(num1)\n",
    "num2 = input('Enter 2nd number:')\n",
    "b = int(num2)\n",
    "\n",
    "print('Initially a =',a,'and b =',b)\n",
    "a = a + b\n",
    "b = a - b\n",
    "a = a - b\n",
    "print('After Swapping a =',a,'and b =',b)"
   ]
