    "# program to find number of zeros in a number\n",
    "\n",
    "num = input('Enter a number:')\n",
    "n = int(num)\n",
    "\n",
    "count = 0\n",
    "\n",
    "while(n):\n",
    "    i = n%10\n",
    "    if(i==0):\n",
    "        count+=1\n",
    "    n=n//10\n",
    "print('The number of zeros in the number is',count)"
