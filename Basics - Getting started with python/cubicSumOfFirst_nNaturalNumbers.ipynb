   "source": [
    "# program  to generate cube sum of first n natural numbers\n",
    "\n",
    "num = input('Enter the number:')\n",
    "n = int(num)\n",
    "\n",
    "summ = 0\n",
    "for i in range(1,n+1):\n",
    "    summ += i**3\n",
    "    \n",
    "print('The cubic sum of first',n,'natural numbers is',summ)"
